#, kde-format
msgid "Add..."
msgstr "Thêm..."

#: filetypedetails.cpp:92
#, kde-format
msgid "Add a new pattern for the selected file type."
msgstr "Thêm một dạng mẫu mới cho kiểu tệp được chọn."

