#: kioclient.cpp:55
#, kde-format
msgid ""
"\n"
"Syntax:\n"
msgstr ""
"\n"
"სინტაქსი:\n"

#: kioclient.cpp:57
#, kde-format
msgctxt "The argument is the command \"kioclient openProperties\""
msgid ""
"  %1 'url'\n"
"            # Opens a properties dialog of 'url'\n"
"\n"
msgstr ""
"  %1 'url'\n"
"            # 'url'-ის თვისებების ფანჯრის გახსნა\n"
"\n"

#: kioclient.cpp:64
#, kde-format
msgctxt "The argument is the command \"kioclient exec\""
msgid ""
"  %1 'url' ['mimetype']\n"
"            # Tries to open the document pointed to by 'url', in the "
"application\n"
"            # associated with it in KDE. You may omit 'mimetype'.\n"
"            # In that case the mimetype is determined automatically.\n"
"            # 'url' can be the URL of a document, a *.desktop file,\n"
"            # or an executable.\n"
msgstr ""
"  %1 'url' ['mimetype']\n"
"            # ცდილობს გახსნას დოკუმენტი, რომელზეც მითითებული ბმული "
"მიუთითითებს, აპლიკაციით\n"
"            # რომელიც მასზე KDE-შია ასოცირებული. შეგიძლიათ გამოტოვოთ "
"'mimetype'.\n"
"            # ამ შემთხვევაში დოკუმენტის mime ტიპი ავტომატურად დადგინდება.\n"
"            # 'url' შეიძლება იყოს ბმული დოკუმენტზე, *.desktop ფაილზე\n"
"            # ან გამშვებ ფაილზე.\n"

#: kioclient.cpp:75
#, kde-format
msgctxt "The argument is the command \"kioclient move\""
msgid ""
"  %1 'src' 'dest'\n"
"            # Moves the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"            #   'dest' may be \"trash:/\" to move the files to the trash.\n"
"            #   The short version 'kioclient mv' is also available.\n"
"\n"
msgstr ""
"  %1 'src' 'dest'\n"
"            # გადააქვს URL 'src'-დან 'dest'-მდე.\n"
"            #   'src' შეიძლება ბმულების სიას წარმოადგენდეს.\n"
"            #   'dest' შეიძლება იყოს \"trash:/\", რაც ფაილებს ნაგავში "
"გადაყრის.\n"
"            #   ასევე ხელმისაწვდომია მოკლე ვერსიაც: 'kioclient mv'.\n"
"\n"

#: kioclient.cpp:85
#, kde-format
msgctxt "The argument is the command \"kioclient download\""
msgid ""
"  %1 ['src']\n"
"            # Copies the URL 'src' to a user-specified location.\n"
"            #   'src' may be a list of URLs, if not present then\n"
"            #   a URL will be requested.\n"
"\n"
msgstr ""
"  %1 ['src']\n"
"            #  ბმულს 'src' მომხმარებლის მიერ მითითებულ ადგილზე აკოპირებს.\n"
"            #   'src' შეიძლება იყოს ბმულების სია. თუ მითითებული არაა,\n"
"            #   URL-ს გკითხავთ.\n"
"\n"

#: kioclient.cpp:94
#, kde-format
msgctxt "The argument is the command \"kioclient copy\""
msgid ""
"  %1 'src' 'dest'\n"
"            # Copies the URL 'src' to 'dest'.\n"
"            #   'src' may be a list of URLs.\n"
"            #   The short version 'kioclient cp' is also available.\n"
"\n"
msgstr ""
"  %1 'src' 'dest'\n"
"            # ბმულს 'src'-დან 'dest'-მდე დააკოპირებს.\n"
"            #   'src' შეიძლება ბმულების სიაც იყოს.\n"
"            #   ასევე ხელმისაწვდომია მოკლე ვერსიაც: 'kioclient cp'..\n"
"\n"

#: kioclient.cpp:103
#, kde-format
msgctxt "The argument is the command \"kioclient cat\""
msgid ""
"  %1 'url'\n"
"            # Prints the contents of the file 'url' to the standard output\n"
"\n"
msgstr ""
"  %1 'url'\n"
"            # ბმულთ მითითებული ფაილის შემცველობის სტანდარტულ გამოტანაზე "
"დაბეჭდვა.\n"
"\n"

#: kioclient.cpp:110
#, kde-format
msgctxt "The argument is the command \"kioclient ls\""
msgid ""
"  %1 'url'\n"
"            # Lists the contents of the directory 'url' to stdout\n"
"\n"
msgstr ""
"  %1 'url'\n"
"            # 'url'-ით მითითებული საქაღალდის შემცველობის სტანდარტულ "
"გამოტანაზე დაბეჭდვა.\n"
"\n"

#: kioclient.cpp:117
#, kde-format
msgctxt "The argument is the command \"kioclient remove\""
msgid ""
"  %1 'url'\n"
"            # Removes the URL\n"
"            #   'url' may be a list of URLs.\n"
"            #   The short version 'kioclient rm' is also available.\n"
"\n"
msgstr ""
"  %1 'url'\n"
"            # URL-ის წაშლა\n"
"            #   'url' შეიძლება ბმულების სიაც იყოს.\n"
"            #   ასევე ხელმისაწვდომია უფრო მოკლე ვარიანტი 'kioclient rm'.\n"
"\n"

#: kioclient.cpp:126
#, kde-format
msgctxt "The argument is the command \"kioclient stat\""
msgid ""
"  %1 'url'\n"
"            # Shows all of the available information for 'url'\n"
"\n"
msgstr ""
"  %1 'url'\n"
"            # 'URL'-ისთვის ხელმისაწვდომი ინფორმაციის ჩვენება\n"
"\n"

#: kioclient.cpp:133
#, kde-format
msgctxt "The argument is the command \"kioclient appmenu\""
msgid ""
"  %1\n"
"            # Opens a basic application launcher\n"
"\n"
msgstr ""
"  %1\n"
"            # ჩვეულებრივი აპლიკაციების გაშვების გახსნა\n"
"\n"

#: kioclient.cpp:139
#, kde-format
msgid "*** Examples:\n"
msgstr "*** მაგალითები:\n"

#: kioclient.cpp:140
#, kde-format
msgid ""
"  kioclient exec file:/home/weis/data/test.html\n"
"             // Opens the file with the default application associated\n"
"             // with this MimeType\n"
"\n"
msgstr ""
"  kioclient exec file:/home/weis/data/test.html\n"
"             // ამ MIME ტიპთან ასოცირებული ნაგულისხმები\n"
"             // აპლიკაციის გაშვება\n"
"\n"

#: kioclient.cpp:145
#, kde-format
msgid ""
"  kioclient exec ftp://localhost/\n"
"             // Opens URL with the default handler for ftp:// scheme\n"
"\n"
msgstr ""
"  kioclient exec ftp://localhost/\n"
"             // URL-ის ftp:// ნაგულისხმებ ბრაუზერში გახსნა\n"
"\n"

#: kioclient.cpp:149
#, kde-format
msgid ""
"  kioclient exec file:/root/Desktop/emacs.desktop\n"
"             // Starts emacs\n"
"\n"
msgstr ""
"  kioclient exec file:/root/Desktop/emacs.desktop\n"
"             // emacs-ის გაშვება\n"
"\n"

#: kioclient.cpp:153
#, kde-format
msgid ""
"  kioclient exec .\n"
"             // Opens the current directory in the default\n"
"             // file manager. Very convenient.\n"
"\n"
msgstr ""
"  kioclient exec .\n"
"             // მიმდინარე საქაღალდის ნაგულისხმებ ფაილების მმართველში\n"
"             // გახსნა. ძალიან მოსახერხებელია\n"
"\n"

