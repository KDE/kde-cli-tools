#: kdesu.cpp:95
#, kde-format
msgid "Geert Jansen"
msgstr "ગીર્ટ જેનસેન"

#: kdesu.cpp:95
#, kde-format
msgid "Maintainer"
msgstr "જાળવનાર"

#: kdesu.cpp:96
